{
  "name": "pivot-pivot",
  "description": "Pivot-Pivot is a data-grid component with pivot-table-like functionality for data display, filtering, and exploration.",
  "version": "1.0.0",
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "browser": "index.js",
  "browserify": {
    "transform": [
      "reactify",
      "envify",
      "cssify"
    ]
  },
  "bugs": {
    "url": "https://github.com/rgbkrk/pivot-pivot/issues"
  },
  "dependencies": {
    "cssify": "^0.7.0",
    "dataframe": "^1.3.0",
    "envify": "^3.2.0",
    "lodash": "^4.1.0",
    "react": ">=0.12.2",
    "reactify": "^1.0.0",
    "wildemitter": "^1.0.1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "budo": "^8.0.3",
    "faker": "^2.1.2",
    "react-dom": "^0.14.7",
    "uglify-js": "^2.4.16"
  },
  "directories": {
    "example": "example"
  },
  "homepage": "https://github.com/rgbkrk/pivot-pivot",
  "keywords": [
    "data",
    "excel",
    "grid",
    "pagination",
    "pivot",
    "react",
    "react-component",
    "sort"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgbkrk/pivot-pivot.git"
  },
  "scripts": {
    "example": "budo example/demo.js --live",
    "example-basic": "budo example/basic.js",
    "example-persist": "budo example/persist.js",
    "dist": "npm run remove-dist && npm run build-standalone",
    "remove-dist": "rm dist/*.js",
    "build-standalone": "NODE_ENV=production browserify lib/umd.js | uglifyjs -mc > dist/react-pivot-standalone-${npm_package_version}.min.js"
  }
}
